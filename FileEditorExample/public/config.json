{
    "$schema": "https://focusany.com/sdk/config.schema.json",
    "name": "FileEditorExample",
    "version": "1.0.0",
    "title": "Editor插件示例",
    "description": "支持文件通用编辑，可以快速实现自定义的文件类型编辑器",
    "author": "FocusAny",
    "homepage": "https://focusany.com",
    "preload": "preload.cjs",
    "logo": "logo.svg",
    "main": "index.html",
    "permissions": [
        "File"
    ],
    "setting": {
    },
    "actions": [
        {
            "name": "default",
            "title": "Editor插件示例",
            "type": "web",
            "matches": [
                "FileEditorExample",
                {
                    "type": "editor",
                    "name": "editor",
                    "extensions": [
                        "fad"
                    ],
                    "fadTypes": [
                        "FileEditorExample"
                    ]
                }
            ]
        }
    ],
    "development": {
        "env": "dev",
        "main": "http://localhost:5173",
        "releaseDoc": "../release.md",
        "contentDoc": "../content.md",
        "previewDoc": "../preview.md"
    }
}
